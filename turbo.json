{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "setup": {
      "cache": false
    },
    "watch": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": [
        "dist/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "typecheck": {},
    "lint": {},
    "check": {
      "dependsOn": [
        "typecheck",
        "lint"
      ]
    },
    "format": {
      "cache": false
    },
    "test:integration": {},
    "test": {
      "dependsOn": [
        "test:integration"
      ]
    },
    "database:migrate:new": {
      "cache": false
    },
    "database:migrate:apply": {
      "cache": false
    }
  }
}