{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "setup": {
      "cache": false
    },
    "env": {
      "cache": false
    },
    "watch": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "check-types": {},
    "lint": {
      "dependsOn": [
        "check-types"
      ]
    },
    "vitest": {
      "dependsOn": [
        "lint"
      ]
    },
    "prettier": {
      "cache": false
    }
  }
}